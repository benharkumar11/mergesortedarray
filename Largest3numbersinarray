let arr = [12, 15, 18, 43, 11, 9, 2]
let size = arr.count
var a = 0, b = 0, c = 0

for i in 0..<size {
    if arr[i] > a {
        c = b
        b = a
        a = arr[i]
    }else if arr[i] > b && arr[i] != a{
        c = b
        b = arr[i]
    }else if arr[i] > c && arr[i] != b && arr[i] != a{
        c = arr[i]
    }
}
print(a,b,c)
